  "author": "@JoaquimSerafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/simple.cache/issues"
  },
  "homepage": "https://github.com/joaquimserafim/simple.cache#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">=12"
  },
  "pre-commit": [
    "test",
    "coverage:check"
