{
  "name": "simple.cache",
  "version": "2.0.3",
  "description": "a simple lru cache",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
